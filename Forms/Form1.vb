                            End If
                        Catch

                        End Try
                    End If
                Catch
                End Try

                nfoFunction.savemovienfo(workingMovieDetails.fileinfo.fullpathandfilename, workingMovieDetails, True)
                Call loadinfofile()

                For f = 0 To fullMovieList.Count - 1
