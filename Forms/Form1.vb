                                                                    Try
                                                                        IO.Directory.CreateDirectory(workingpath)
                                                                        destsorted = True
                                                                    Catch ex As Exception

                                                                    End Try
                                                                Else
                                                                    destsorted = True
                                                                End If
                                                                If destsorted = True Then
                                                                    Try
                                                                        Dim filename As String = newactor.actorname.Replace(" ", "_")
