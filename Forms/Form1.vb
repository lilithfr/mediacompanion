            messbox.Close()
            Exit Sub
        End If
        renamelog = renamelog & nfofilestorename.Count & " nfo's to rename" & vbCrLf & vbCrLf
        For Each renamefile In nfofilestorename
            Dim seasonno As String = ""
            Dim episodetitle As String = ""
            Dim showtitle As String = ""
